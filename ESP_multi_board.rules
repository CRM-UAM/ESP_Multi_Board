
(rules PCB ESP_multi_board
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 2079)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.6)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.6)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_600:400_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_600:400_um-power" "Via[0-1]_600:400_um" power
  )
  (via 
    "Via[0-1]_800:400_um-power" "Via[0-1]_800:400_um" power
  )
  (via 
    "Via[0-1]_600:400_um" "Via[0-1]_600:400_um" default
  )
  (via 
    "Via[0-1]_600:400_um-kicad_default" "Via[0-1]_600:400_um" "kicad_default"
  )
  (via_rule
    power "Via[0-1]_800:400_um-power"
  )
  (via_rule
    default "Via[0-1]_600:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_600:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    VDD GND VCC "Net-(P101-Pad2)" M1+ "M1-" "ENC1_A" "ENC1_B"
    M2+ "M2-" "ENC2_A" "ENC2_B" "Net-(P105-Pad1)" "Net-(P105-Pad2)" "Net-(P105-Pad3)" "Net-(P105-Pad4)"
    "Net-(P105-Pad5)" "Net-(P105-Pad6)" "Net-(P105-Pad7)" "Net-(P105-Pad8)" "Net-(P105-Pad9)" "Net-(P105-Pad10)" "Net-(P105-Pad11)" "Net-(P105-Pad12)"
    TX RX GPIO0 SDA SCL "Net-(R101-Pad2)" "Net-(R102-Pad2)" "M1_A"
    "M1_B" "M2_B" "M2_A" "Net-(JP101-Pad1)" "Net-(JP101-Pad3)" "Net-(P110-Pad3)" "Net-(P110-Pad4)" "Net-(P110-Pad5)"
    "Net-(P110-Pad6)" EN1 EN2
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class power
    (clearance_class power)
    (via_rule power)
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)